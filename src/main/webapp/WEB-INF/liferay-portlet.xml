<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>archivemanager-self-service</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>
	</portlet>
	<portlet>
		<portlet-name>archivemanager-user-space</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>
	</portlet>
	<portlet>
		<portlet-name>archivemanager-user-signin</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.personalization.configuration.UserSignInConfigurationAction</configuration-action-class>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>
	</portlet>
	<portlet>
		<portlet-name>archivemanager-search</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.search.configuration.SearchConfigurationAction</configuration-action-class>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>2</render-weight>
	</portlet>
	<portlet>
		<portlet-name>archivemanager-display</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.search.configuration.DisplayConfigurationAction</configuration-action-class>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>2</render-weight>		
	</portlet>
	<portlet>
		<portlet-name>archivemanager-associations</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.search.configuration.AssociationsConfigurationAction</configuration-action-class>
		<instanceable>true</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>		
	</portlet>
	<portlet>
		<portlet-name>archivemanager-attributes</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.search.configuration.AttributesConfigurationAction</configuration-action-class>
		<instanceable>true</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>		
	</portlet>
	<portlet>
		<portlet-name>archivemanager-breadcrumb</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>org.archivemanager.portal.portlet.search.configuration.BreadcrumbConfigurationAction</configuration-action-class>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>		
	</portlet>
	<portlet>
		<portlet-name>archivemanager-digital-object</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<private-session-attributes>false</private-session-attributes>
		<render-weight>1</render-weight>		
	</portlet>
	<!-- APPS -->
	<portlet>
		<portlet-name>toolbar</portlet-name>
		<icon>/icon.png</icon>
	</portlet>
	<portlet>
		<portlet-name>collections</portlet-name>
		<icon>/icon.png</icon>     
		<instanceable>false</instanceable>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>data</portlet-name>
		<icon>/icon.png</icon>
		<control-panel-entry-category>apps</control-panel-entry-category>
        <control-panel-entry-weight>203.0</control-panel-entry-weight>
		<instanceable>false</instanceable>
		<add-default-resource>true</add-default-resource>
	</portlet>
	
	<!-- DOCUMENTATION -->
	<portlet>
		<portlet-name>archivemanager-documentation</portlet-name>
		<icon>/icon.png</icon>
		<private-session-attributes>false</private-session-attributes>
	</portlet>
	
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>